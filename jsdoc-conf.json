{
  "source": {
    "exclude": [
      "./node_modules/"
    ],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "encoding": "utf8",
    "recurse": true,
    "private": false,
    "destination": "./docs",
    "template": "./node_modules/docdash"
  },
  "tags": {
    "allowUnknownTags": true
  },
  "templates": {
    "linenums": true,
    "default": {
      "cleverLinks": true,
      "monospaceLinks": true,
      "outputSourceFiles": false
    }
  },
  "docdash": {
    "search": true,
    "collapse": true
  }
}
